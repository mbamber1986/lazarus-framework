        "php": "^8.0.2",
        "vlucas/phpdotenv": "^5.2",
        "laminas/laminas-diactoros": "^2.5",
        "phpmailer/phpmailer": "^6.2",
        "miladrahimi/phprouter": "^5.1"
    }

}